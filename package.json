  "private": true,
  "scripts": {
    "postinstall": "tsc --build & npm run web-build",
    "start": "npm run env-prod & node ./server/build/index.js",
    "web-dev": "npm run env-dev && ts-node-dev --ignore-watch ./web --respawn --transpileOnly  ./server/src/index.ts",
    "web-next": "next ./web",
    "web-build": "npm run env-prod & next build ./web",
    "mobile-start": "node node_modules/react-native/local-cli/cli.js start",
    "run-android": "react-native run-android",
    "env-dev": "node environments --dev",
    "env-prod": "node environments --prod",
    "env-test": "node environments --test",
    "lint": "eslint -c .eslintrc --ext .js,.ts ./",
    "lint-fix": "eslint -c .eslintrc --ext .js,.ts ./ --fix",
    "test": "npm run env-test && jest"
  },
